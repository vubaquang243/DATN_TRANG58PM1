package com.seatech.tp.bccocaunhadautu.form;

import javax.servlet.http.HttpServletRequest;

import org.apache.struts.action.ActionForm;
import org.apache.struts.action.ActionMapping;

public class BCCoCauNhaDauTuForm extends ActionForm {
    private String chi_tieu = "";
    private String khoi_luong = "";
    private String ty_trong = "";
    private String ngay_phat_hanh_tu_ngay;
    private String ngay_phat_hanh_den_ngay;
    private String loai_tien;
    private String pageNumber;

    public BCCoCauNhaDauTuForm() {
        this.chi_tieu = "";
        this.khoi_luong = "";
        this.ty_trong = "";
        this.ngay_phat_hanh_tu_ngay = "";
        this.ngay_phat_hanh_den_ngay = "";
    }

    @Override
    public void reset(ActionMapping mapping, HttpServletRequest request) {
        super.reset(mapping, request);
        this.chi_tieu = "";
        this.khoi_luong = "";
        this.ty_trong = "";
        this.ngay_phat_hanh_tu_ngay = "";
        this.ngay_phat_hanh_den_ngay = "";
    }

    public void setChi_tieu(String chi_tieu) {
        this.chi_tieu = chi_tieu;
    }

    public String getChi_tieu() {
        return chi_tieu;
    }

    public void setKhoi_luong(String khoi_luong) {
        this.khoi_luong = khoi_luong;
    }

    public String getKhoi_luong() {
        return khoi_luong;
    }

    public void setTy_trong(String ty_trong) {
        this.ty_trong = ty_trong;
    }

    public String getTy_trong() {
        return ty_trong;
    }

    public void setNgay_phat_hanh_tu_ngay(String ngay_phat_hanh_tu_ngay) {
        this.ngay_phat_hanh_tu_ngay = ngay_phat_hanh_tu_ngay;
    }

    public String getNgay_phat_hanh_tu_ngay() {
        return ngay_phat_hanh_tu_ngay;
    }

    public void setNgay_phat_hanh_den_ngay(String ngay_phat_hanh_den_ngay) {
        this.ngay_phat_hanh_den_ngay = ngay_phat_hanh_den_ngay;
    }

    public String getNgay_phat_hanh_den_ngay() {
        return ngay_phat_hanh_den_ngay;
    }

    public void setPageNumber(String pageNumber) {
        this.pageNumber = pageNumber;
    }

    public String getPageNumber() {
        return pageNumber;
    }

    public void setLoai_tien(String loai_tien) {
        this.loai_tien = loai_tien;
    }

    public String getLoai_tien() {
        return loai_tien;
    }
}
